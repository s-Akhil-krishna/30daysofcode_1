#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int getNthFib(int n);
int fib(vector<int> memory,int n);

int getNthFib(int n) {
vector<int> memory(n+1);
	for(int i=0;i<n+1;i++)
		memory[i]=-1;
	memory[1]=0;
	if(n>1)
	memory[2]=1;
	int answer=fib(memory,n);
return answer;
}

int fib(vector<int> memory,int n)
{
	if(memory[n]>=0)
		return memory[n];
	else
	{
		memory[n]= fib(memory,n-1)+fib(memory,n-2);
		return memory[n];
	}
}
